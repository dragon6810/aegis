# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  Awol_config = debug
endif
ifeq ($(config),release)
  Awol_config = release
endif

PROJECTS := Awol

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

Awol:
ifneq (,$(Awol_config))
	@echo "==== Building Awol ($(Awol_config)) ===="
	@${MAKE} --no-print-directory -C . -f Awol.make config=$(Awol_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f Awol.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   Awol"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"